{
  "name": "vc",
  "version": "1.0.0",
  "description": "A git cli for humans",
  "scripts": {
    "test": "jest",
    "start": "env TEST_DIR='./test' ts-node src/vc.ts",
    "clean": "rm -rf dist/ bundle/",
    "compile": "tsc --build tsconfig.json",
    "build": "run-s clean compile",
    "package": "pkg dist/vc.js --config package.json --options max_old_space_size=4096 --targets node14-macos-x64,node14-linux-x64 --out-path bundle/",
    "bundle": "run-s build package"
  },
  "keywords": [],
  "author": "Alexander Heimbuch <github@heimbu.ch>",
  "license": "MIT",
  "dependencies": {
    "@nozzlegear/railway": "2.0.0",
    "chalk": "4.1.1",
    "commander": "7.2.0",
    "enquirer": "2.3.6",
    "execa": "^5.1.1",
    "ora": "5.4.0",
    "simple-git": "2.39.0",
    "date-fns": "2.22.1",
    "human-interval": "1.0.0",
    "fs-extra": "10.0.0",
    "conf": "10.0.1"
  },
  "devDependencies": {
    "ts-node": "10.0.0",
    "typescript": "4.3.2",
    "@types/node": "15.12.1",
    "npm-run-all": "4.1.5",
    "pkg": "5.3.0",
    "ts-jest": "27.0.3",
    "jest": "27.0.6",
    "@types/jest": "26.0.24",
    "tmp-promise": "3.0.2"
  }
}
